# Supabase (required)
# Get these from your Supabase project dashboard → Settings → API
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Meta Graph API Credentials
# Get these from Meta Business Manager → Business Settings

# System User Access Token (with ads_management, ads_read, business_management, pages_read_engagement)
VITE_META_ACCESS_TOKEN=

# Ad Account ID (numeric, without 'act_' prefix)
VITE_META_AD_ACCOUNT_ID=

# Facebook Page ID
VITE_META_PAGE_ID=

# Instagram Account ID (optional)
VITE_META_INSTAGRAM_ACCOUNT_ID=
